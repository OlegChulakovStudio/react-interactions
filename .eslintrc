{
  "extends": "react-app",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "new-cap": [2, { "capIsNewExceptions": ["List", "Map", "Set", "GSAP"] }],
    "react/no-multi-comp": 0,
    "import/default": 0,
    "import/no-duplicates": 0,
    "import/named": 0,
    "import/namespace": 0,
    "import/no-unresolved": 0,
    "import/no-named-as-default": 2,
    "jsx-a11y/href-no-hash": 0,
    "comma-dangle": 0,  // not sure why airbnb turned this on. gross!
    "indent": ["error", "tab"],
    "no-console": 0,
    "no-alert": 0,
    "no-param-reassign": 0,
    "func-names": 0,
    "id-length": [2, { "min": 1 }]
  },
  "plugins": [
    "react", "import"
  ],
  "settings": {
    "import/parser": "babel-eslint",
    "import/resolve": {
      "moduleDirectory": ["node_modules", "src"]
    }
  }
}